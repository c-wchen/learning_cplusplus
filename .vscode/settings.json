{
    "files.associations": {
        "iostream": "cpp",
        "ostream": "cpp",
        "iosfwd": "cpp",
        "fstream": "cpp",
        "istream": "cpp",
        "*.tcc": "cpp",
        "stdio.h": "c"
    },
    "C_Cpp.errorSquiggles": "Disabled",
    "editor.tokenColorCustomizations": {
        "comments": "#8ba596",
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": [
        "cplusplus_debug"
    ]
}